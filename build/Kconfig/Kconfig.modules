menu "sniot_zephyr_template (/home/seatech/my-workspace/sniot_zephyr_template)"
osource "/home/seatech/my-workspace/sniot_zephyr_template/Kconfig"
config ZEPHYR_SNIOT_ZEPHYR_TEMPLATE_MODULE
	bool
	default y
endmenu
